// capture global.Promise before it may potentially be overwritten

var Promise = global.Promise; // see ES2015 spec 25.4.4.5, https://stackoverflow.com/a/38339199

var isPromise = function isPromise(candidate) {
  return Promise.resolve(candidate) === candidate;
};

var _default = isPromise;
exports["default"] = _default;
